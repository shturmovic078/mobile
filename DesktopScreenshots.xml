<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
 
<suite name="Browser Print " verbose="1">
    <parameter name="param" value="Test"/>
    
     <test name="Print on the browser Safari">
        <parameter name="browser" value="safari"/>
        <classes>
            <class name="brand.automation.DesktopScreenshots"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="Print on browser Firefox 35">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="brand.automation.DesktopScreenshots"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="Print on browser  IE10">
